var targetElement = document.querySelector('.store-footer__top');
var lineBreak = document.createElement("br");
textNode.style.textAlign = "center";

var test= window.location.href; 
console.log(test);
for ( var counter = 0 ; counter < category_link.length ; counter++)
{

    if ( test  === category_link[counter]) {

        // console.log("111------> " + test);
        console.log("222------> " + category_link[counter]);
        // console.log("successful connection" );
        if (targetElement) {
            textNode.textContent = category_desc[counter];
            targetElement.insertAdjacentElement('beforebegin', lineBreak);
            targetElement.insertAdjacentElement('beforebegin', textNode);
        }
    }
}



