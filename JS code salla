var giftCards = document.querySelector('#1477959471');
console.log("giftCards:: ", giftCards);

// Check if the parent element exists
if (giftCards) {
    // Select the child anchor tag
    const anchorTag = giftCards.querySelector('a');
    console.log("anchorTag :: ", anchorTag );

    // Check if the anchor tag exists
    if (anchorTag) {
        // Edit the href attribute
        anchorTag.href = `${anchorTag.href}`;
    } else {
        console.error('Anchor tag not found under the parent element.');
    }
} else {
    console.error('Parent element not found.');
}

var textNode = document.createElement("div");
var targetElement = document.querySelector('.store-footer__top');
var lineBreak = document.createElement("br");
textNode.style.textAlign = "center";

var test= window.location.href; 
// console.log(test);
console.log("maheeeeeeeeeeeeeeer");
for ( var counter = 0 ; counter < category_link.length ; counter++)
{

    if ( test  === category_link[counter]) {
        console.log("111------> " + test);
        console.log("222------> " + category_link[counter]);
        console.log("successful connection" );
        if (targetElement) {
            textNode.textContent = category_desc[counter];
            targetElement.insertAdjacentElement('beforebegin', lineBreak);
            targetElement.insertAdjacentElement('beforebegin', textNode);
        }
    }
}





