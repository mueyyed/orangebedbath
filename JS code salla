];


var category_link = [
    "https://orangebedbathksa.com/%D8%B9%D8%B1%D9%88%D8%B6-%D8%AE%D8%A7%D8%B5%D8%A9/c1295862587?filters[category_id]=1295862587",
    "https://orangebedbathksa.com/%D8%B9%D8%B1%D9%88%D8%B6-%D8%AE%D8%A7%D8%B5%D8%A9/c1295862587?filters[category_id]=608632329"
];


function extractCategoryNumber(url) {
    const regex = /\d+/;

    const parts = url.split(/\/c\d+\?/);

    const match = parts[1].match(regex);

    return match ? match[0] : null;
}

var textNode = document.createElement("div");
var targetElement = document.querySelector('.store-footer__top');
var lineBreak = document.createElement("br");
textNode.style.textAlign = "center";

var test= extractCategoryNumber(window.location.href); 
console.log(test + "from link window");
for ( var counter = 0 ; counter < category_link.length ; counter++)
{
    console.log(extractCategoryNumber(category_link[counter]) + "from list of links categories");
    if ( test  === extractCategoryNumber(category_link[counter])) {
        // console.log("111------> " + test);
        // console.log("222------> " + category_link[counter]);
        console.log("successful connection" );
        if (targetElement) {
            textNode.textContent = category_desc[counter];
            targetElement.insertAdjacentElement('beforebegin', lineBreak);
            targetElement.insertAdjacentElement('beforebegin', textNode);
        }
    }
}

