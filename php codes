/* 2024.07.08 */
<?php

add_filter( 'woocommerce_currencies', 'add_my_currency' );

function add_my_currency( $currencies ) {
     $currencies['SARI'] = __( 'Currency name', 'woocommerce' );
     return $currencies;
}


## Edited by Eng.Moayyad 2023.11.28
/* add refund button */
add_filter('woocommerce_my_account_my_orders_actions', 'wpsh_order_refund_button', 9999, 2);
/* Add "Order again" button to Woocommerce my account page*/
function wpsh_order_refund_button($actions, $order) {
    // Check if the order status is either 'delivered' or 'processing'
    if ($order->has_status('delivered')){
        $actions['order-again'] = array(
            'url'  => 'https://www.orangebedbath.com/return/',
            'name' => 'طلب استرجاع',
        );
    }
    return $actions;
}

/* 31/03/2024 select by filteration */
 add_filter('woocommerce_shipping_package_name', 'change_shipping_label_checkout', 10, 3);
function change_shipping_label_checkout($package_name, $i, $package) {
    $package_name = 'طريقة الشحن'; 
    return $package_name;
}



/*remove cancel button status */
add_filter('woocommerce_my_account_my_orders_actions', 'remove_myaccount_orders_cancel_button', 10, 2);
function remove_myaccount_orders_cancel_button( $actions, $order ){
    unset($actions['cancel']);

    return $actions;
}


