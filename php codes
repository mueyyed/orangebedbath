/* Add Payment method Column to orders */
add_filter( 'manage_edit-shop_order_columns', 'add_payment_method_column', 20 );
function add_payment_method_column( $columns ) {
 $new_columns = array();
 foreach ( $columns as $column_name => $column_info ) {
 $new_columns[ $column_name ] = $column_info;
 if ( 'order_total' === $column_name ) {
 $new_columns['order_payment'] = __( 'Payment Method', 'my-textdomain' );
 }
 }
 return $new_columns;
}
add_action( 'manage_shop_order_posts_custom_column', 'add_payment_method_column_content' );
function add_payment_method_column_content( $column ) {
 global $post;
 if ( 'order_payment' === $column ) {
 $order = wc_get_order( $post->ID );
 echo $order->payment_method_title;
 }
}

/* Customize Invoice */

function print_inovice_customize() {
    ?>
<style>
	@font-face {
		font-family: 'Orange';
		font-style: normal;
		font-weight: 400;
		src: url(../wp-content/themes/woodmart-child/frutigerltarabic-45light.eot);
		src: url(../wp-content/themes/woodmart-child/frutigerltarabic-45light.eot?#iefix) format('embedded-opentype'),
			url(../wp-content/themes/woodmart-child/frutigerltarabic-45light.woff) format('woff'),
			url(../wp-content/themes/woodmart-child/frutigerltarabic-45light.ttf) format('truetype');
	}

	#page {
		text-align: right;
		direction: rtl;
		font-family: 'Orange', sans-serif !important;
	}

	th {
		text-align: right;
	}

	.order-items dt,
	.order-items dd {
		float: right;
	}

	.colophon-policies {
		display: grid;
		grid-template-columns: 1fr;
	}

	.colophon-policies div {
		padding: 10px;
	}

	.colophon-policies p,
	.colophon-policies li  {
		font-size: 12px;
		line-height: 1.5em;
	}

	.company-logo img{
		height:100px;
		width:auto;
	}
	.order-branding{
		margin-bottom: 1em;
	}
    .company-logo{
    display:grid;
    grid-template-columns:1fr 1fr;
    }
    .barcode{
    text-align:center;
    }
    .barcode img{
    max-width:200px;
    }
</style>
<?php
}
add_action( 'wcdn_head', 'print_inovice_customize', 20 ); 

setcookie(TEST_COOKIE, 'WP Cookie check', 0, COOKIEPATH, COOKIE_DOMAIN);
if ( SITECOOKIEPATH != COOKIEPATH ) setcookie(TEST_COOKIE, 'WP Cookie check', 0, SITECOOKIEPATH, COOKIE_DOMAIN);


/**
 * Add SKU to PDF order heading
 */
add_filter ( 'pdf_template_table_headings' , 'custom_pdf_table_headers' );

function custom_pdf_table_headers() {

    $headers =  '<table class="pdf_table_row_heading shop_table orderdetails" width="100%">' . 
                '<thead>' .
                '<tr>' .
				'<th class="pdf_table_cell_heading" width="10%" valign="top" align="center">' . __( 'SKU', PDFLANGUAGE )         . '</th>' .                   
                '<th class="pdf_table_cell_heading" width="7%" valign="top" align="center">'  . __( 'Qty', PDFLANGUAGE )        . '</th>' . 
                '<th class="pdf_table_cell_heading" width="50%" valign="top" align="center">'  . __( 'Product', PDFLANGUAGE )    . '</th>' .
                '<th class="pdf_table_cell_heading" width="13%" valign="top" align="center">'  . __( 'Price Ex', PDFLANGUAGE )   . '</th>' .
                '<th class="pdf_table_cell_heading" width="10%" valign="top" align="center">'  . __( 'Tax', PDFLANGUAGE )        . '</th>' .
                '<th class="pdf_table_cell_heading" width="15%" valign="top" align="center">' . __( 'Total Inc', PDFLANGUAGE )  . '</th>' .
                '</tr>' .
                '</thead>' .
                '</table>';

    return $headers;

}

/**
 * Add SKU to order line
 */
add_filter ( 'pdf_template_line_output' , 'custom_pdf_lines', 10, 2 );

function custom_pdf_lines ( $line, $order_id ) {
    global $woocommerce;
    $order   = new WC_Order( $order_id );

    // Check WC version – changes for WC 3.0.0
    $pre_wc_30      = version_compare( WC_VERSION, '3.0', '<' );
    $order_currency = $pre_wc_30 ? $order->get_order_currency() : $order->get_currency();
                
    $pdflines  = '<table width="100%">';
    $pdflines .= '<tbody>';

    if ( sizeof( $order->get_items() ) > 0 ) {

        foreach ( $order->get_items() as $item ) {                  
            
            if ( $item['qty'] ) {
                
                $line = '';
                // $item_loop++;

                $_product   = $order->get_product_from_item( $item );
                $item_name  = $item['name'];
                $item_id    = $pre_wc_30 ? $item['variation_id'] : $item->get_id();

                $meta_display = '';
                if ( version_compare( WC_VERSION, '3.0', '<' ) ) {
                    $item_meta  = new WC_Order_Item_Meta( $item );
                    $meta_display = $item_meta->display( true, true );
                    $meta_display = $meta_display ? ( ' ( ' . $meta_display . ' )' ) : '';
                } else {
                    foreach ( $item->get_formatted_meta_data() as $meta_key => $meta ) {
                        $meta_display .= '<br /><small>(' . $meta->display_key . ':' . wp_kses_post( strip_tags( $meta->display_value ) ) . ')</small>';
                    }
                }

                if ( $meta_display ) {

                    $meta_output     = apply_filters( 'pdf_invoice_meta_output', $meta_display );
                    $item_name      .= $meta_output;

                }
                
                $line =     '<tr>' .
							'<td valign="top" width="10%" align="center">' .  $_product->get_sku() . '</td>' .
                            '<td valign="top" width="7%" align="center">' . $item['qty'] . ' x</td>' .
                            '<td valign="top" width="50%" align="center">' .  stripslashes( $item_name ) . '</td>' .
                            '<td valign="top" width="13%" align="center">'  .  wc_price( $item['line_subtotal'] / $item['qty'], array( 'currency' => $order_currency ) ) . '</td>' .                          
                            '<td valign="top" width="10%" align="center">'  .  wc_price( $item['line_subtotal_tax'] / $item['qty'], array( 'currency' => $order_currency ) ). '</td>' .           
                            '<td valign="top" width="15%" align="center">' .  wc_price( $item['line_subtotal'] + $item['line_subtotal_tax'], array( 'currency' => $order_currency ) ). '</td>' .
                            '</tr>';
                
                $pdflines .= $line;
            }

        }

    }

    $pdflines .=    '</tbody>';
    $pdflines .=    '</table>';

    return $pdflines;
}

/**
 * Halt the main query in the case of an empty search 
 */
add_filter( 'posts_search', function( $search, \WP_Query $q )
{
    if( ! is_admin() && empty( $search ) && $q->is_search() && $q->is_main_query() )
        $search .=" AND 0=1 ";

    return $search;
}, 10, 2 );

remove_action( 'woocommerce_before_checkout_form', 'woocommerce_checkout_coupon_form', 10 );
add_action( 'woocommerce_review_order_after_cart_contents', 'woocommerce_checkout_coupon_form_custom' );
function woocommerce_checkout_coupon_form_custom() {
    echo '<tr class="coupon-form"><td colspan="2">';
    
    wc_get_template(
        'checkout/form-coupon.php',
        array(
            'checkout' => WC()->checkout(),
        )
    );
    echo '</tr></td>';
}



